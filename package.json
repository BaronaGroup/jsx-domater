{
  "name": "jsx-domater",
  "description": "Create DOM nodes using JSX, with jsdom support",
  "version": "1.0.0",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.14",
    "@types/jsdom": "12.2.3",
    "@types/node": "10.10.1",
    "gulp": "3.9.1",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "5.0.1",
    "jest": "23.6.0",
    "jsdom": "12.2.0",
    "npm-release": "1.0.0",
    "ts-jest": "23.1.4",
    "tslint": "5.16.0",
    "typescript": "3.4.5"
  },
  "scripts": {
    "test": "jest",
    "tslint": "tslint --project tsconfig.json",
    "tslintfix": "npm run tslint -- --fix",
    "lint": "npm run tslint",
    "build": "gulp build",
    "gulp": "gulp",
    "prepare": "npm run gulp dist",
    "release": "npm test && npm-release"
  },
  "jest": {
    "testMatch": [
      "**/*-test.ts*"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "node",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "resetModules": true
  },
  "homepage": "https://github.com/BaronaGroup/jsx-domater",
  "repository": {
    "type": "git",
    "url": "BaronaGroup/jsx-domater"
  },
  "license": "MIT",
  "author": {
    "name": "Tapani Haka"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/jsx",
  "types": "dist/jsx.d.ts"
}
