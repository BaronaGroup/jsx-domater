    "@types/node": "^10.10.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.0-alpha.3",
    "jest": "^23.6.0",
    "jsdom": "^12.0.0",
